    "outline_primary": "Use for primary CTAs on light backgrounds",
    "outline_primary_green_hover": "Use for primary CTAs with green brand accent",
    "outline_secondary": "Use for secondary actions on light backgrounds",
    "outline_secondary_green_hover": "Use for secondary actions with green brand accent",
    "outline_white": "Use on dark/hero backgrounds",
