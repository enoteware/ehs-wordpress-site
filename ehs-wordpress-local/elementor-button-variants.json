  "elementor_global_widget_instructions": {
    "step_1": "In Elementor, go to Site Settings > Global Widgets",
    "step_2": "Create a new Global Widget for each button variant",
    "step_3": "Apply the settings from the JSON above to each widget",
    "step_4": "Save as Global Widget with the variant name",
    "step_5": "Use the Global Widget throughout your site for consistency"
  }
